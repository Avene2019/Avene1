ackage com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a= sc.nextInt();
        int b = sc.nextInt();
        int c = calculus(a,b);
        System.out.println(c);
    }

    private static int calculus(int a, int b){
        int sum = a+b;
        return sum;
    }
}
